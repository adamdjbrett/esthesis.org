<div class="container mt-3">
<nav class="nav justify-content-center">
{% for key, item in wordpress.config.navheaderlist %}
    {% set border_class = "" %}
    {% if not loop.last %}
        {% set border_class = " border-end" %}
    {% endif %}
    <a class="nav-link{{ border_class }}" href="{{ item.url }}">{{ item.nav }}</a>
{% else %}
    <a class="nav-link" href="{{wordpress.config.url}}">Home</a>
{% endfor %}
</nav>
</div>